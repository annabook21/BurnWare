# BurnWare Frontend Environment Variables

# AWS Region
VITE_AWS_REGION=us-east-1

# Cognito
VITE_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# API Base URL
VITE_API_BASE_URL=https://api.burnware.example.com

# AppSync Events (real-time messages). From AppSync stack outputs when deploying via CDK.
VITE_APPSYNC_HTTP_DOMAIN=
VITE_APPSYNC_REALTIME_DOMAIN=
VITE_APPSYNC_API_KEY=

# Optional: Enable debug logging
VITE_DEBUG=false
